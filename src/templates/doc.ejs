<!DOCTYPE html>
<html>
    <head>
        <style>
        table {
            border-collapse: collapse;
        }
        table, th, td {
            border: 1px solid black;
        }
        </style>
    </head>
    <body>
        <div style="width: 85%; margin: 0 7.5%;">
            <div style="text-align: center; padding-left: 50px;">            
                <h3>Test</h5>
                <table>
                <% if (data.length) { %>
                    <% data.forEach(item => {%>
                       <tr>
                           <td><%= item.name %></td>
                           <td><%= item.email %></td>
                           <td><%= item.city %></td>
                           <td><%= item.country %></td>
                        </tr>
                    <%  });%>
                <% }%>
                </table>
             </div>
        </div>
    </body>
</html>